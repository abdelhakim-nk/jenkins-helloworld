pipeline {
    agent any
    
    stages{
        stage('build and run') {
            steps { 
                git branch: 'main', url: 'https://github.com/abdelhakim-nk/jenkins-helloworld.git', credentialsId: 'tokenPassGit'
                bat '''
                javac Main.java
                java Main
                 '''
            }
        }
    
    }
}


    
